<h1>Admins</h1>

<table class = "table table-bordered table-striped table table-hover">
  <tr>
	<th>Username</th>
	<th>First Name</th>
	<th>Last Name</th>
	<th>Street</th>
	<th>City</th>
	<th>State</th>
	<th>Zip</th>
	<th>Area Code</th>
	<th>Phone Number</th>
	<th>TYPE</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @admins.each do |admin| %>
  <tr>
	<td><%= admin.email %></td>
	<td><%= admin.first_name %></td>
	<td><%= admin.last_name %></td>
	<td><%= admin.street %></td>
	<td><%= admin.city %></td>
	<td><%= admin.state %></td>
	<td><%= admin.zip %></td>
	<td><%= admin.area_code %></td>
	<td><%= admin.phone_number %></td>
	<td><%= admin.type %></td>
	<td><%= link_to image_tag('show.png'), admin %></td>
	<td><%= link_to image_tag('penciledit.png'), edit_admin_path(admin) %></td>
	<td><%= link_to image_tag('delete.jpg'), admin, method: :delete, data: {confirm: 'Are you sure?'}%></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Admin', new_admin_path %>
