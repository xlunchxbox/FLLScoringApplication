<h1>Core Values</h1>
<table class = "table table-bordered table-striped table table-hover table-condensed">
  <tr>
	<th><%= sortable "teamNumber", "Team #" %></th>
    <th><%= sortable "discovery", "D" %></th>
    <th><%= sortable "teamSpirit", "TS" %></th>
    <th><%= sortable "integration", "Integ" %></th>
    <th><%= sortable "effectiveness", "Effec" %></th>
    <th><%= sortable "efficiency", "Effic" %></th>
    <th><%= sortable "kidsDoTheWork", "KDTW" %></th>
    <th><%= sortable "inclustion", "Inc" %></th>
    <th><%= sortable "respect", "R" %></th>
    <th><%= sortable "coopertition", "C" %></th>
    <th><%= sortable "inspirationTotal", "IT" %></th>
    <th><%= sortable "teamworkTotal", "TwT" %></th>
    <th><%= sortable "graciousProfessionalismTotal", "GPT" %></th>
    <th><%= sortable "coreValueTotal", "CVT" %></th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @corevalues.each do |corevalue| %>
  <tr>
    <td><%= corevalue.teamNumber %></td>
    <td><%= corevalue.discovery %></td>
    <td><%= corevalue.teamSpirit %></td>
    <td><%= corevalue.integration %></td>
    <td><%= corevalue.effectiveness %></td>
    <td><%= corevalue.efficiency %></td>
    <td><%= corevalue.kidsDoTheWork %></td>
    <td><%= corevalue.inclusion %></td>
    <td><%= corevalue.respect %></td>
    <td><%= corevalue.coopertition %></td>
    <td><%= corevalue.inspirationTotal %></td>
    <td><%= corevalue.teamworkTotal %></td>
    <td><%= corevalue.graciousProfessionalismTotal %></td>
    <td><%= corevalue.coreValueTotal %></td>
    <%# link_to 'Show', corevalue %>
    <%# link_to 'Edit', edit_corevalue_path(corevalue) %>
    <%# link_to 'Destroy', corevalue, method: :delete, data: { confirm: 'Are you sure?' } %>
	<td><%= link_to image_tag('show.png'), corevalue %></td>
	<td><%= link_to image_tag('penciledit.png'), edit_corevalue_path(corevalue) %></td>
	<td><%= link_to image_tag('delete.jpg'), corevalue, method: :delete, data: {confirm: 'Are you sure?'}%></td>
  </tr>
<% end %>
</table>
<br />

<%= link_to 'New Corevalue', new_corevalue_path %>
