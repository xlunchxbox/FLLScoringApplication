<h1>Judges</h1>

<table class = "table table-bordered table-striped table table-hover table-condensed">
  <tr>
  	<th>Email</th>
	<th>First Name</th>
	<th>Last Name</th>
	<th>Street</th>
	<th>City</th>
	<th>State</th>
	<th>Zip</th>
	<th>Area Code</th>
	<th>Phone Number</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @judges.each do |judge| %>
  <tr>
  	<td><%= judge.email %></td>
	<td><%= judge.firstName %></td>
	<td><%= judge.lastName %></td>
	<td><%= judge.street %></td>
	<td><%= judge.city %></td>
	<td><%= judge.state %></td>
	<td><%= judge.zip %></td>
	<td><%= judge.areaCode %></td>
	<td><%= judge.phoneNumber %></td>
    <%# link_to 'Show', judge %>
    <%# link_to 'Edit', edit_judge_path(judge) %>
    <%# link_to 'Destroy', judge, method: :delete, data: { confirm: 'Are you sure?' } %>
	<td><%= link_to image_tag('show.png'), judge %></td>
	<td><%= link_to image_tag('penciledit.png'), edit_judge_path(judge) %></td>
	<td><%= link_to image_tag('delete.jpg'), judge, method: :delete, data: {confirm: 'Are you sure?'}%></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Judge', new_judge_path %>
